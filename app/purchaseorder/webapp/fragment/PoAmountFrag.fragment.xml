<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core">
	<Popover id="_IDGenPopover1" showHeader="false" class="sapUiContentPadding" placement="Top">
		<Table id="DetailAmountId" inset="false" fixedLayout="false">
			<columns>
				<Column id="_IDGenColumn1">
					<Text id="_IDGenText1" text="HSN/SAC"/>
				</Column>
				<Column id="_IDGenColumn2">
					<Text id="_IDGenText2" text="IGST"/>
				</Column>
				<Column id="_IDGenColumn3">
					<Text id="_IDGenText3" text="CGST"/>
				</Column>
				<Column id="_IDGenColumn4">
					<Text id="_IDGenText4" text="SGST/UGST"/>
				</Column>
			</columns>
			<items>
				<ColumnListItem id="_IDGenColumnListItem1">
					<cells>
						<Text id="_IDGenText5" text="{/Hsn_Code}"/>
						<Text id="_IDGenText6" text="{path:'/Igst', formatter:'sp.fiori.purchaseorder.controller.formatter.getPercentSign'}"/>
						<Text id="_IDGenText7" text="{path:'/Cgst', formatter:'sp.fiori.purchaseorder.controller.formatter.getPercentSign'}"/>
						<Text id="_IDGenText8" text="{path:'/Sgst', formatter:'sp.fiori.purchaseorder.controller.formatter.getPercentSign'}"/>
					</cells>
				</ColumnListItem>
			</items>
		</Table>
	</Popover>
</core:FragmentDefinition>